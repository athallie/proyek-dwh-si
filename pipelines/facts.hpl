  <transform>
    <name>Order Table</name>
    <type>TableInput</type>
    <description/>
    <distribute>Y</distribute>
    <custom_distribution/>
    <copies>1</copies>
    <partitioning>
      <method>none</method>
      <schema_name/>
    </partitioning>
    <connection>Gravity Book Store</connection>
    <execute_each_row>N</execute_each_row>
    <limit>0</limit>
    <sql>SELECT
    c.customer_id,
    b.book_id,
    YEAR(oh.status_date) AS year,
    MONTH(oh.status_date) AS month,  
    DAY(oh.status_date) AS day,
    HOUR(oh.status_date) AS hour,
    MINUTE(oh.status_date) AS minute,
    SECOND(oh.status_date) AS second,
    s.method_id,
    SUM(ol.price) AS revenue_book_sales
FROM
    cust_order o
JOIN
    order_line ol ON o.order_id = ol.order_id
JOIN
    customer c ON o.customer_id = c.customer_id
JOIN
    book b ON ol.book_id = b.book_id
JOIN
    shipping_method s ON o.shipping_method_id = s.method_id
JOIN
    order_history oh ON o.order_id = oh.order_id  
JOIN
    order_status os ON oh.status_id = os.status_id  
WHERE
    os.status_id = 4  
GROUP BY
    c.customer_id,
    b.book_id,
    oh.status_date,
    s.method_id
ORDER BY
    c.customer_id,
    b.book_id,
    oh.status_date</sql>
    <variables_active>N</variables_active>
